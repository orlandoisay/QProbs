{
	"manifest_version": 2,
	"name":  "QProbs",
	"version": "0.1",

	"description": "QProbs is a simple tool for the Codeforces's users.",

	"browser_action" : {
		"default_icon": { 
			"16": "images/icon16.png",
			"32": "images/icon32.png",
			"64": "images/icon64.png"
		},
		"default_title": "QProbs",
		"default_popup": "popup.html"
	},

	"author": "Orlando Isay Mendoza G.",
	"permissions": [
		"activeTab",
		"tabs"
	]
}